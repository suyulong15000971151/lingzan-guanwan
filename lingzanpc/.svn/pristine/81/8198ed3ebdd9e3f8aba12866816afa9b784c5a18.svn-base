body{background:#f6f6f6}
/*这是search搜索区域*/
.search{width:100%;height:200px;overflow:;background: #fff;box-sizing: border-box;border-bottom:1px solid #d0d0d0;box-sizing: border-box;padding-top:30px}
.search h1{font-size: 23px;text-align: center;}
.search .titleBox { font-size: 23px; text-align: center;}
.search .search-box{width:600px;height:80px;margin:5px auto 0}
.search .search-box .search-nav{width:100%;height:50px;box-sizing: border-box;border:1px solid #d5d5d5;display: flex;position: relative;}
.search .search-box .search-nav .search-select{position: relative;display: inline-block;width:90px;font-size: 18px;height:50px;box-sizing: border-box;border-right:1px solid #d5d5d5;text-align: center;line-height: 50px;}
.search .search-box .search-nav .search-select i{display: inline-block;position: absolute;right:5px;bottom:5px;border-bottom: 4px solid #055cfc;border-right:4px solid #055cfc;border-top:4px solid transparent;border-left:4px solid transparent}
.search .search-box .search-nav .search-select .search-option{width:90px;overflow: hidden;box-shadow: 0 0 4px rgba(0,0,0,.7);background:#fff;position: absolute;top:55px;display: none; z-index: 100;}
.search .search-box .search-nav .search-select .search-option .search-item{width:100%;height:40px}
.search .search-box .search-nav .search-select .search-option .search-item:hover{background:#eee}
.search .search-box .search-nav a.subData{position: absolute;right:0}

.search .search-box .search-nav input{outline: none;border:none;line-height: 50px;flex: 1;font-size: 16px;box-sizing: border-box;}

.search .search-box .search-sub{width:100%;height:25px;box-sizing: border-box;font-size: 15px;color:#919191;margin-top:10px;position: relative;}
.search .search-box .search-sub span{margin: 0 10px 0;}
.search .search-box .search-sub a{margin: 0 2px 0;color:#919191}
.search .search-box .search-sub a:hover{text-decoration: underline;color:#C90000}
.search .search-box .search-sub .hot{display: inline-block;width:450px;height:20px;vertical-align: top;overflow: hidden;}
.search .search-box .search-sub .change{position: absolute;width:20px;height:20px;position: absolute;top:0;right:0;background:url(../image/ch.png) no-repeat;background-size: 20px 20px;}
/*这是中间标题*/
    .middle .title{overflow: hidden;padding-left:20px;box-sizing: border-box;}
   .middle .title h1{height:100px;font-size:28px;color:#000;line-height: 100px;background:url(../image/line.png) no-repeat center left;text-indent: 25px;}
.pagination-one{display: block;}
/*.pagination-one:after{content: '';display: inline-block;clear: both;width:1px}*/
/*瀑布流*/
 .container { position: relative;margin: 0 auto;}
  /*.container:after{content: '';display: inline-block;clear: both;}*/
.container .box {float: left;-webkit-transition: left .3s ease-in-out, top .3s ease-in-out;background-color: #fff;margin: 10px 10px;border-radius: 5px;overflow: hidden;}
.container .box:hover{box-shadow:0 5px 5px #999;}
.container .box .boximg{position: relative;}
.container .box .boximg i{display: none;width:50px;height:30px;background:#055cfc;text-align: center;line-height: 30px;color:#fff;font-size: 15px;position: absolute;top:13px;right:13px;border-radius: 5px;opacity: 0;}
.container .box .boximg .col-tow{display: inline-block;width:50px;height:30px;background:#055cfc;text-align: center;line-height: 30px;color:#fff;font-size: 15px;position: absolute;top:13px;right:13px;border-radius: 5px;opacity: 0;}
.container .box a{display: block;}
.container .box a:hover .boximg>img:hover{opacity: 0.8;}
.container .box .boximg p{font-size:15px;color:#000;text-indent:10px;}
.container .box .boximg em{width:60px;height:20px;display: inline-block;line-height: 20px;text-align: center;background: #b9b9b9;font-size: 15px;color:#fff;border-radius: 5px;margin:20px 10px 0}
.container .box .boximg .rate{opacity: 0;display: inline-block;width:50px;height:30px;background:#d43636;text-align: center;line-height: 30px;font-size: 15px;position: absolute;top:10px;right:10px;color:#fff;border-radius: 5px;}
.container .box .boximg .rate1{opacity: 0;display: inline-block;width:50px;height:30px;background:#909090;text-align: center;line-height: 30px;font-size: 15px;position: absolute;top:10px;right:10px;color:#fff;border-radius: 5px;}
.container .box a:hover .rate{opacity: 1;}
.container .box a:hover .rate1{opacity: 1;}
.container .box img {width: 255px;padding-bottom: 10px;}
.container .box .behavior{width:90%;margin:30px auto 0;border-top:1px solid #eee}
.container .box .behavior .peson{display: inline-block;overflow: hidden;}
.container .box .behavior .peson img{width:40px;height:40px;border-radius: 50%;display: inline-block;float:left;margin-top:10px}
.container .box .peson span{display: inline-block;line-height: 40px;float:left;margin-top:10px;margin-left:10px}

.container .box .behavior .praise{display: inline-block;overflow: hidden;float:right}
.container .box .behavior .praise span{display: inline-block;width:20px;height:20px;background:url(../image/zan.png) no-repeat;float:left;margin-top:15px;margin-right:10px}
.container .box .behavior .praise .yizan{display: inline-block;width:20px;height:20px;background:url(../image/yizan.png) no-repeat;float:left;margin-top:15px;margin-right:10px}
.container .box .behavior .praise strong{display: inline-block;width:20px;height:20px;text-align: center;line-height: 20px;float:left;margin-top:15px;margin-right:0px;color:#989898}
.container .box .behavior .praise em{display: inline-block;height:20px;font-size:15px;line-height: 20px;color:#000;float:left;margin-top: 15px;opacity: 1;}
.container .box .behavior .praise i{display: inline-block;height:20px;font-size:15px;line-height: 20px;color:#000;float:left;margin-top: 15px;opacity: 1;}

.add-btn{
            width: 100%;
            background-color: #ff7d3c;
            height: 50px;
            line-height: 50px;
            text-align: center;
            font-size: 30px;
            font-weight: 700;
            position: absolute;
            color: #fff;
            display: none;
            
        }
        
/*这里是搜索框里特效*/
.demo-info{
	padding:0 0 12px 0;
}
.demo-tip{
	display:none;
}
.label-top{
    display: block;
    height: 22px;
    line-height: 22px;
    vertical-align: middle;
}
.textbox{border:none}


/*这里是page-tow内容*/
.page-tow{display: none;}
.page-tow .container .box{width:255px;background:#fff;padding-bottom: 10px;}
.page-tow .container .box .boximg img{width:80px;height:80px;display: block;margin:20px auto 10px;border-radius: 50%;}
.page-tow .container .box .boximg p{font-size: 16px;line-height: 20px;text-align: center;}
.page-tow .container .box .behavior{width:95%;overflow: hidden;margin: 0 auto 0;border:none}
.page-tow .container .box .behavior .personr{line-height: 40px;text-align: center;margin:10px 0 0;display: block;height:40px;box-sizing: border-box;width:100%;border-top:1px solid #eee;border-bottom: 1px solid #eee;}
.page-tow .container .box .show-img{overflow: hidden;width:100%;height:70px;margin:10px 0;}
.page-tow .container .box .show-img ul{overflow: hidden;width:270px}
.page-tow .container .box .show-img ul li{float:left;}
.page-tow .container .box .show-img ul li img{width:70px;height:70px;margin-right:15px}
.page-tow .container .box .interest{width:100%;height:30px;background:#f4f4f4;text-align: center;font-size: 16px;line-height: 30px;}

.page-tow .middle .title h1{display: inline-block;}
.page-tow .middle .title span{display: inline-block;font-size: 15px;margin-left:10px}
.tagbox-label{ font-size: 15px;}


/*成功的弹窗*/
#win{width:100%;height:100%;position: absolute;top:0;left;0;background:rgba(0,0,0,.6);z-index: 999999;display: none;}
.anima{animation: alls 0.5s ease-in-out;}
#win .win-box{width:100%;height:100%;position: relative;}
#win .win-box .com{width:250px;height:100px;background:#fff;position: absolute;top:50%;left:50%;transform: translate(-50%,-50%);}
#win .win-box .com img{width:40px;height:40px;float:left;margin:30px 100px;}
#win .win-box .com i{display: inline-block;width:12px;height:12px;position: absolute;top:10px;right:10px;background:url(../image/delete_03.png) no-repeat;background-size: 100% 100%;float:right}
#win .win-box .com span{text-align: center;}

@keyframes alls{
    0%{transform: scale(0);}
    100%{transform: scale(1);}
    /*100%{transform: scale(0);}*/
}