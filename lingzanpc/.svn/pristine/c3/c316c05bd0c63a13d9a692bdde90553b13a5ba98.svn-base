/*这是项目标题*/
.sub-nav{height:80px;width:100%;background:#f6f6f6;overflow: hidden;box-sizing: border-box;padding:0 30px 0;border-bottom:1px solid #d0d0d0}
.sub-nav h1{line-height: 80px;font-size:22px;display: inline-block;float:left;height:80px;text-indent: 20px;background:url(../image/line.png) no-repeat center left;}
.sub-nav select{display: inline-block;float:left;line-height: 40px;font-size: 16px;color:#000;margin:0 40px 0;height:40px;box-sizing: border-box;margin-top:20px;font-weight: 600;}
.sub-nav i{display: inline-block;float:right;line-height: 80px;font-size: 18px;color:#000;font-weight: 600;text-indent: 40px;background:url(../image/back.png) no-repeat center left;cursor: pointer;}
.sub-nav span{display: inline-block;border: 1px solid #ccc;box-sizing: border-box;width:70px;line-height: 40px;text-indent: 32px;height:40px;float:right;margin:20px;background:url(../image/sh.png) no-repeat 10px center;background-size: 20px 22px;}
.sub-nav .files{width:132px;border:1px solid #ccc;height:40px;box-sizing: border-box;display: inline-block;float:right;margin:20px 10px}
.sub-nav .files a{display: inline-block;width:30px;height:40px}
.sub-nav .files p{text-align: center;line-height: 40px;display: inline-block;font-size: 12px;width:70px}
.sub-nav .files a.add{display: inline-block;float:left;line-height: 40px;font-size: 22px;font-weight: 600;text-align: center;color:#000}
.sub-nav .files a.load{display: inline-block;float:right;}
.sub-nav .files a.load img{display: block;margin:9px auto 0}
.sub-nav .files p{border-left:1px solid #ccc;border-right:1px solid #ccc;box-sizing: border-box;}



.product{margin:0 auto 0;clear: both;width:1200px;position: relative;z-index: 9;}
.product .left{width:64.3%;display: inline-block;float:left;position:relative;overflow: hidden;}
.product .left .item{width:170px;overflow: hidden;display: inline-block;float:left;margin:20px 0px 0 0px;}
.product .left .item ul{overflow: hidden;width:210px;height:440px}
.product .left .item ul li{width:80px;overflow: hidden;height:80px;margin-bottom:9px;float:left;box-sizing: border-box;border:1px solid #ccc;margin-right:10px;position: relative;}
.product .left .item ul li.increase{background:url(../image/crease.png) no-repeat center}
.product .left .item ul li.increase input{display: block;height:100%;opacity: 0;}
.product .left .item ul li .bg{display: block;width:100%;height:100%}
.product .left .item ul li i{width:15px;height:15px;display: none;position: absolute;right:0;top:0;background:url(../image/xx.png) no-repeat;background-size: 100% 100%;}
.product .left .item ul li:hover i{display: block;}
.product .left .item button{outline: none;border:none;background:#47a447;color:#fff;width:70px;height:20px;border-radius: 2px;line-height: 20px;text-align: center;}
.product .left .item ul li em{display: block;position: absolute;bottom:0;left:0;width:0;background:rgb(100,149,198);height:10px}
.product .left .item ul li em.progess{animation: pro 1s linear;}

.product .left .item ul li img{width:100%;height:100%}
.product .left .show{width:580px;display: inline-block;float:left;margin-top:20px;margin-bottom:20px;margin-left:10px;overflow: hidden;box-sizing: border-box;border:1px solid #ccc;height:435px;overflow: hidden;}
.product .left .show img{display: block;margin:0 auto 0}
.product .left a.arror-l{width:20px;height:35px;background:url(../image/arrorl.png) no-repeat;background-size: 100% 100%;position: absolute;left:10px;top:300px}
.product .left a.arror-r{width:20px;height:35px;background:url(../image/arrorr.png) no-repeat;background-size: 100% 100%;position: absolute;left:920px;top:300px}
.product .right{width:35%;display: inline-block;float:left;overflow: hidden;margin-top:20px;height:435px;box-sizing: border-box;border:1px solid #ccc}
.product .right .title{box-sizing: border-box;padding-right:10px;border-bottom:1px solid #eee;overflow: hidden;}
.product .right .title h1{margin-left:10px;font-size: 18px;color:#000;line-height: 30px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;display: inline-block;padding-top:0px}
.product .right .title span{display: inline-block;float:right;height:100px;line-height: 100px;text-indent: 30px;font-size:18px;background:url(../image/share.png) no-repeat center left;margin-right:30px;font-weight: 500;}
.product .right .title p{font-size:13px;line-height: 20px;box-sizing: border-box;padding-left:10px;margin-bottom: 5px;height:40px;overflow: hidden;width:250px;display: inline-block;}
.product .right .title img{width:30px;height:30px;border-radius: 50%;display: inline-block;margin-left:10px;vertical-align: middle;}
.product .right .title em{line-height: 30px;text-indent: 5px;display: inline-block;}
.product .right .title .pso{display: inline-block;overflow: hidden;float:right;margin-top:10px}
/*加关注*/
.product .right .follow{overflow: hidden;border-bottom:1px solid #eee;height:50px;display: none;}
.product .right .follow .follow-l{overflow: hidden;display: inline-block;float:left}
.product .right .follow .follow-l img{display: inline-block;width:40px;height:40px;border-radius: 50%;float:left;margin:5px 10px 0}
.product .right .follow .follow-l span{line-height: 50px;display: inline-block;font-size: 13px;}
.product .right .follow .follow-r{overflow: hidden;display: inline-block;float:right;line-height: 80px;font-size: 16px;margin-right:50px}

/*这里是邀请项目成员*/
.members{width:100%;padding-left:10px;display: block;clear: both;position: relative;height:150px}
.members:after{content: "";display: block;clear: both;}
.members .member{display: inline-block;float:left;position: absolute;left:50%;transform: translateX(-50%);}
.members .member h4{font-size: 14px;line-height: 25px;margin:10px 0;text-align: center;}
.members .member .member-add{}
.members .member .member-add img{width:40px;height:40px;display: inline-block;vertical-align: middle;margin-right:5px}
.members .member .member-add span{display: inline-block;line-height: 40px;color:#666;font-size: 13px;}
.members .member .member-add .peon-item{display: inline-block;width:40px;height:40px;margin:0 5px 0;position:relative}
.members .member .member-add .peon-item img{border-radius: 50%;}
.members .member .member-add .peon-item .people-id{width:270px;height:300px;background:#fff;box-shadow: 0 0 5px rgba(0,0,0,.7);box-sizing: border-box;padding-top:10px;position:absolute;top:50%;left:50%;z-index: 9999;display: none;}
.members .member .member-add .peon-item .people-id .rol{width:250px;height:275px;margin:0 auto 0}
.members .member .member-add .peon-item .people-id .rol .left{width:100px;height:100%;display: inline-block;float:left;box-sizing: border-box;border-right:1px solid #eee}
.members .member .member-add .peon-item .people-id .rol .left img{display: block;margin:20px auto 10px;width:60px;height:60px}
.members .member .member-add .peon-item .people-id .rol .left p{text-align: center;height:30px;line-height: 30px;font-size: 16px;}

.members .member .member-add .peon-item .people-id .rol .right{width:150px;height:100%;display: inline-block;float:left;font-size: 12px;border:none}
.members .member .member-add .peon-item .people-id .rol .right span{display: inline-block;width:80px;white-space: nowrap;font-size: 12px;height:30px;line-height: 30px;}
.members .member .member-add .peon-item .people-id .rol .right input.ed{display: inline-block;width:100px;height:20px;text-indent: 5px;background:#eee;border:1px solid #ccc;outline: none;}
.members .member .member-add .peon-item .people-id .rol .right input{display: inline-block;width:100px;height:20px;text-indent: 5px;background:#fff;border:none;outline: none;}
.members .member .member-add .peon-item .people-id .rol .right label{margin-left:5px;margin-right:5px}
.members .member .member-add .peon-item .people-id .rol .right .Jurisdiction{width:100%;overflow: hidden;}
.members .member .member-add .peon-item .people-id .rol .right .Jurisdiction .explain{width:30px;height:100%;float:left;display: inline-block;}
.members .member .member-add .peon-item .people-id .rol .right .Jurisdiction .explain label{display: inline-block;line-height: 25px;width:30px}
.members .member .member-add .peon-item .people-id .rol .right .Jurisdiction .rules{margin-left:35px;}
.members .member .member-add .peon-item .people-id .rol .right .Jurisdiction .rules{line-height: 25px;text-indent: 25px;}
.members .member .member-add .peon-item .people-id .rol .right .Jurisdiction .rules p{color:#8b8b8b;height:25px;text-indent: 5px;}
.members .member .member-add .peon-item .people-id .rol .right .Jurisdiction .rules p.judje{background:url(../image/wei.png) no-repeat right center;}
.members .member .member-add .peon-item .people-id .rol .right .Jurisdiction .rules p i{display: none;width:12px;height:12px;border:1px solid #c0c0c0;float:left;margin:6px 5px 0 5px;background:#eee}
.members .member .member-add .peon-item .people-id .rol .right .Jurisdiction .rules p i.gou{background:url(../image/gou.png) no-repeat;background-size: 100% 100%;}
.members .member .member-add .peon-item .people-id .rol .right .set a{color:#000;display: inline-block;padding:0 7px;background:#eee;margin:0 10px;border-radius: 4px;}
.members .member .member-add .peon-item .people-id .rol .right .set a:hover{background:#055cfc;color:#fff}


.members .member .member-add .peon-item .people-id .rol .right .set{height:25px;overflow: hidden;text-align: center;line-height: 25px;margin-top:10px}
.members .member .member-add .peon-item .people-id .rol .right .set i{display: inline-block;width:40px;height:25px;background:#eeeeee}
.members .member .member-add .peon-item .people-id .rol .right .set i:hover{background:#055cfc;color:#fff}
.members .member .member-add .peon-item .people-id .rol .right .set span:hover{background:#055cfc;color:#fff}
.members .member .member-add .peon-item .people-id .rol .right .set em:hover{background:#055cfc;color:#fff}
.members .member .member-add .peon-item .people-id .rol .right .set span{display: inline-block;width:40px;height:25px;background:#eeeeee;line-height: 25px;}
.members .member .member-add .peon-item .people-id .rol .right .set em{display: inline-block;width:40px;height:25px;background:#eeeeee}

.members .member-select{float:right;display: inline-block;margin:10px 30px 0 0;display:none}
.members .member-select select{height:30px;font-size: 16px;color:#000;line-height: 30px;display: inline-block;}
.members .member-select select option['selected'=true]{background: #000;}
/*评论留言*/
.product .right .message{overflow: hidden;}
.product .right .message .leave{height:40px;line-height: 40px;color:#000;font-size: 14px;text-indent: 10px;margin-top:10px;overflow: hidden;}
.product .right .message .leave i{color:#989898;margin-left:10px}
.product .right .message .leave a{float:right;font-size: 13px;color:#055cfc;margin:10px 40px 0;text-decoration: underline;}
.product .right .message .ping{overflow: hidden;display: flex;}
.product .right .message .ping input[type="text"]{height:40px;flex: 1;display: inline-block;margin:0 10px 0 10px}
.product .right .message .ping button{display: inline-block;height:40px;background:red;width:70px;display: inline;outline: none;margin-right:50px;border: none;background:url(../image/fasong_03.png) no-repeat;background-size: 100% 100%;}
.product .right .message .person{overflow: hidden;}
.product .right .message .person ul{overflow: hidden;}
.product .right .message .person ul li{width:95%;border-bottom: 1px solid #eee;margin:0 auto 0;display: flex;padding:5px 0}
.product .right .message .person ul li .pson{display: inline-block;height:100%;float:left;overflow: hidden;}
.product .right .message .person ul li .pson img{width:30px;height:30px;float:left;border-radius: 50%;margin-right:20px}
.product .right .message .person ul li .text{display: inline-block;float:right;flex: 1;font-size: 12px;line-height: 25px;}
.product .right .message .person ul li .text span{color:#989898}
.product .right .message .person strong{display: inline-block;width:80px;height:20px;line-height: 20px;;color:#000;float:right;text-align: center;margin-right:30px;margin-top:10px;margin-bottom:20px;text-decoration: underline;}
/*这里是清单*/
.detail{width:100%;background:#ebebeb;overflow: hidden;}
.detail .detail-box{width:90%;margin:20px 0 0 110px;overflow: hidden;}
.detail .detail-box .detail-title{width:100%;height:80px;overflow: hidden;box-sizing: border-box;border-bottom:1px solid #989898}
.detail .detail-box .detail-title .pro{display: inline-block;}
.detail .detail-box .detail-title .pro h1{font-size: 25px;line-height: 80px;text-indent: 20px;display: inline-block;background:url(../image/line.png) no-repeat center left;background-size: 15px 23px;}
