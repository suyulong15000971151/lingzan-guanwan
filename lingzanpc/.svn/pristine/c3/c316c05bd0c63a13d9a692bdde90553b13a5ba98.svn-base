.detail .detail-box .detail-item .thead .adjust{width:80px;text-align: center;display: inline-block;margin:0 15px 0 10px;float:left}
.detail .detail-box .detail-item .thead .offer{width:80px;text-align: center;display: inline-block;margin:0 0px 0 10px;float:left}
.detail .detail-box .detail-item .thead .count{width:80px;text-align: center;display: inline-block;margin:0 -5px 0 0px;float:left}
.detail .detail-box .detail-item .thead .subtotal{width:80px;text-align: center;display: inline-block;float:left;margin-left:-15px}
.detail .detail-box .detail-item .thead .operation{width:80px;text-align: center;display: inline-block;;float:left;margin-left:15px}

.detail .detail-box .detail-item ul{width:100%;}
.detail .detail-box .detail-item ul li{width:100%;border-bottom:1px solid #dadada;clear: both;padding:10px 0;}
.detail .detail-box .detail-item ul li .author{width:100%;height:80px;position: relative;}
.detail .detail-box .detail-item ul li .all-select{width:30px;height:100%;overflow: hidden;display: inline-block;float:left}
