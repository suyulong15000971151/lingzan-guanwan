.detail .detail-box .detail-title .change span{display: inline-block;float:right;height:40px;background:url(../image/jiawen_03.png) no-repeat center center;font-size: 15px;color:#000;line-height: 40px;width:30px}
.detail .detail-box .detail-title .change em{display: inline-block;float:right;height:40px;background:url(../image/jiawen_03.png) no-repeat center left;color:#fff;font-size: 14px;line-height: 40px;margin:0px 10px 0;width:70px;text-indent: 25px;color:#000}
.detail .detail-box .detail-title .change i{display: inline-block;float:right;line-height:40px;width:30px;height:40px;background:url(../image/change.png) no-repeat center left;background-size: 30px 12px;text-indent: 35px;}
.detail .detail-box .detail-title .print{height:40px;display: inline-block;float:right;margin:20px 20px;text-indent: 40px;background:url(../image/primer.png) no-repeat center center;line-height: 40px;width:30px}
.detail .detail-box .detail-title .export{height:40px;display: inline-block;float:right;margin:20px 0px;text-indent: 40px;background:url(../image/export.png) no-repeat center center;line-height: 40px;width:30px}
.detail .detail-box .detail-title .notes{height:40px;display: inline-block;float:left;margin:20px 20px 10px;text-indent: 40px;background:url(../image/record_03.png) no-repeat center center;line-height: 40px;width:35px}

.detail .detail-box .detail-title .change-edit{display: inline-block;float:right;height:100%;overflow: hidden;margin-top:20px;display: none;}
