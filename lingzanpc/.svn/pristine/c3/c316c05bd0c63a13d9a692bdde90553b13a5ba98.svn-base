.detail .detail-box .detail-item ul li .remark{display: inline-block;float:left;width:160px;height:80px;overflow: hidden;margin:0 20px;box-sizing: border-box;padding-top:10px}
.detail .detail-box .detail-item ul li .remark input{width:148px;height:30px;box-sizing: border-box;font-size: 12px;outline: none;border:1px solid #acacac;text-indent: 5px;}
.detail .detail-box .detail-item ul li .remark input.number{width:148px;margin-right:14px;margin-bottom:5px}
.detail .detail-box .detail-item ul li .remark input.name{width:148px;}
.detail .detail-box .detail-item ul li .remark input.notes{display:none;}
.detail .detail-box .detail-item ul li .pros{width:220px;height:80px;display: inline-block;float:left}
.detail .detail-box .detail-item ul li .pros img{width:65px;height:65px;display: inline-block;float:left;margin:10px 5px}
