/*这里是帮他调整弹窗*/
#help{width:100%;height:100%;z-index: 99999;background:rgba(0,0,0,.5);position: fixed;top:0;left:0;display: none;}
#help .help-box{width:350px;height:350px;background:#fff;position: fixed;left:50%;top:50%;transform: translate(-50%,-50%);}
#help .help-box .title{width:100%;height:50px;border-bottom: 1px solid #ccc;box-sizing: border-box;line-height: 50px;}
#help .help-box .title span{display: inline-block;text-indent: 20px;font-size: 15px;font-weight: 600;}
#help .help-box .title i{display: inline-block;width:12px;height:12px;float:right;background:url(../image/delete_03.png) no-repeat;background-size: 100% 100%;margin:19px}
#help .help-box .con{width:100%;height:220px;background:#fff;overflow: hidden;box-sizing: border-box;padding-top:40px;border-bottom: 1px solid #ccc;}
#help .help-box .con .help-item{width:100%;overflow: hidden;margin-bottom:20px}
#help .help-box .con .help-item .plan{width:180px;height:30px;color:#fff;text-align: center;line-height: 30px;background:#055cfc;border-radius: 5px;margin:0 auto 10px}
#help .help-box .con .help-item p{font-size: 12px;text-align: center;color:#9a9a9a}
#help .help-box .sub{width:100%;overflow: hidden;height:80px;background:#f3f3f3;box-sizing: border-box;padding:25px}
#help .help-box .sub button{outline: none;border: none;display: block;width:180px;height:30px;line-height: 30px;text-align: center;background:#cecece;color:#000;margin:0 auto 0;border-radius: 5px;}


/*这里是订单记录弹窗*/
#record{position: absolute;top:0;left:0;z-index: 9999999;width:100%;height:100%;background:rgba(0,0,0,.6);display:none}
#record .record-box{width:100%;height:100%;position: relative;}
