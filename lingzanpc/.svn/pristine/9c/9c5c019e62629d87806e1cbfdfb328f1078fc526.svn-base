body{background:#f6f6f6}
/*这是banner区域*/
.banner{width:100%;height:270px;background:url(../image/images/banner2_02.jpg) no-repeat;background-size: 100% 100%;overflow: hidden;}
.banner h1{text-align: center;font-size: 32px;color:#000;padding:40px 0 30px}
.banner .project{overflow: hidden;text-align: center;}
.banner .project a{display: inline-block;width:100px;height:100px;position: relative;}
.banner .project a p{height:40px;line-height: 40px;text-align: center;font-size: 15px;color:#fff;position: absolute;bottom:0;width:100%}
.banner .project a img{width:100px;height:100px}
/*这是中间标题*/
    .middle .title{overflow: hidden;padding-left:20px;box-sizing: border-box;display: inline-block;}    
   .middle .title h1{height:100px;font-size:28px;color:#000;line-height: 100px;background:url(../image/line.png) no-repeat center left;text-indent: 25px;display: inline-block;}
   .middle .title span{display: inline-block;height:100px;line-height: 100px;font-size: 15px;font-weight: 600;margin-left:20px}
       .middle .key{display: inline-block;float:right;line-height: 100px;font-size: 15px;font-weight: 600;}
       .middle .key i{display: inline-block;width:20px;height:20px;background: url(../image/search_03.png) no-repeat;background-size: 100% 100%;float:right;margin:40px 30px 0 15px}
/*瀑布流*/
 .container { position: relative;margin: 0 auto;}
.container .box {float: left;-webkit-transition: left .3s ease-in-out, top .3s ease-in-out;background-color: #fff;margin: 10px 10px;border-radius: 5px;overflow: hidden;}
.container .box:hover{box-shadow:0 5px 5px #999;}
.container .box .boximg{position: relative;}
.container .box .boximg i{display: none;width:50px;height:30px;background:#055cfc;text-align: center;line-height: 30px;color:#fff;font-size: 15px;position: absolute;top:13px;right:13px;border-radius: 5px;opacity: 0;}
.container .box .boximg .col-tow{display: inline-block;width:50px;height:30px;background:#055cfc;text-align: center;line-height: 30px;color:#fff;font-size: 15px;position: absolute;top:13px;right:13px;border-radius: 5px;opacity: 0;}
.container .box .boximg p{font-size:15px;color:#000;text-indent:10px;}
.container .box .boximg em{width:60px;height:20px;display: inline-block;line-height: 20px;text-align: center;background: #b9b9b9;font-size: 15px;color:#fff;border-radius: 5px;margin:20px 10px 0}
.container .box img {width: 255px;padding-bottom: 10px;}
.container .box a:hover .boximg>img:hover{opacity: 0.8;}
.container .box .behavior{width:90%;margin:30px auto 0;border-top:1px solid #eee}
.container .box .behavior .peson{display: inline-block;overflow: hidden;}
.container .box .behavior .peson img{width:40px;height:40px;border-radius: 50%;display: inline-block;float:left;margin-top:10px}
.container .box .peson span{display: inline-block;line-height: 40px;float:left;margin-top:10px;margin-left:10px}
.container .box .behavior .praise{display: inline-block;overflow: hidden;float:right}
.container .box .behavior .praise span{display: inline-block;width:20px;height:20px;background:url(../image/zan.png) no-repeat;float:left;margin-top:15px;margin-right:10px}
.container .box .behavior .praise strong{display: inline-block;width:20px;height:20px;text-align: center;line-height: 20px;float:left;margin-top:15px;margin-right:0px;color:#989898}
.container .box .behavior .praise i{display: inline-block;height:20px;font-size:15px;line-height: 20px;color:#000;float:left;margin-top: 15px;}
.container .box .behavior .praise em{display: inline-block;height:20px;font-size:15px;line-height: 20px;color:#000;float:left;margin-top: 15px;}
.container .box a{display: block;}

.add-btn{
            width: 100%;
            background-color: #ff7d3c;
            height: 50px;
            line-height: 50px;
            text-align: center;
            font-size: 30px;
            font-weight: 700;
            position: absolute;
            color: #fff;
            display: none;
        }