body{background:#f6f6f6}
/*这是banner区域*/
.manpeople{width:100%;height:200px;border-bottom: 1px solid #eee;background:#fff}
.manpeople .man-box{width:300px;height:150px;margin:0 auto 0;overflow: hidden;display: flex;box-sizing: border-box;padding-top:30px}
.manpeople .man-box .potato{display: inline-block;width:100px;height:100%;float:left;margin-right:10px}
.manpeople .man-box .potato img{width:100px;height:100px;border-radius: 50%;}
.manpeople .man-box .explain{display: inline-block;float:left;flex: 1;}
.manpeople .man-box .explain h2{font-size: 23px;margin:10px 0 5px}
.manpeople .man-box .explain p{font-size: 14px;color:#a2a2a2;margin-bottom:5px}
.manpeople .man-box .explain strong{}
.manpeople .man-box .explain i{display: inline-block;width:20px;height:20px;background:url(../image/vip.png) no-repeat;background-size:100% 100%;vertical-align: middle;margin-right:5px}
/*这里是个人导航也*/
.man-nav{width:100%;height:100px;background:#fff;overflow: hidden;}
.man-nav .navigation{overflow: hidden;width:100%;height:100%;text-align: center;}
.man-nav .navigation a{display: inline-block;height:100%;font-size: 18px;line-height: 25px;color:#919191;padding:0 40px}
.man-nav .navigation a span{margin-top:20px;display: inline-block;}
/*这里是下面展示的内容*/

.man-show .plus{position: relative;}

.man-show{width:100%;overflow: hidden;background:#f6f6f6;padding:30px 0 0;width:95%;margin:0 auto 0;padding-bottom:40px}
.man-show .man-item{width:250px;height:320px;background:#fff;display: inline-block;float:left;border:1px solid #eae9e9;margin-right:20px;overflow: hidden;position: relative;padding-bottom:10px}
.man-show .man-item .edit{margin-top:10px;width:110px;height:30px;font-size:14px;line-height: 30px;text-align: center;border:1px solid #dcdcdc;box-sizing: border-box;background:#f4f4f4;display: inline-block;float:left}
.man-show .man-item .remove{margin-top:10px;width:110px;height:30px;font-size:14px;line-height: 30px;text-align: center;border:1px solid #dcdcdc;box-sizing: border-box;background:#f4f4f4;display: inline-block;float:right}
.man-show .man-item p{text-align: center;font-size: 14px;height:30px;box-sizing: border-box;border-top:1px solid #eee;border-bottom:1px solid #eee;color:#999;line-height: 30px;margin-bottom:10px}
.man-show .man-item .item-box{width:230px;overflow: hidden;display: block;margin:10px auto 0;}
.man-show .man-item .item-box h4{font-size:16px;line-height: 25px;margin-bottom: 5px;text-align: center;}
.man-show .man-item .item-box .big-img{width:100%;height:150px;overflow: hidden;margin-bottom:5px}
.man-show .man-item .item-box .big-img img{width:100px;height:100px;display:block;margin:10px auto 10px;border-radius: 50%;}
.man-show .man-item .item-box .small{width:260px;overflow: hidden;}
.man-show .man-item .item-box .small img{display: inline-block;float:left;width:70px;height:70px;margin-right:10px}
.man-show .man-item .state{position: absolute;right:-100px;top:10px;width:100%;height:30px;background: #055cfc;text-align: center;color:#fff;line-height: 30px;transform: rotate(45deg);transform-origin:center ;}
.man-show .man-item .production{background:#000;}
.man-show .man-item .win{background:#ababab;}



.man-show .plus p{height:40px;line-height: 40px;text-indent: 50px;position: absolute;top:50%;transform: translateY(-50%);border:0;margin:0;width:100%;font-size: 16px;color:#919191;background:url(../image/friends.png) no-repeat 70px center;background-size:40px 30px;}

.add-btn{
            width: 100%;
            background-color: #ff7d3c;
            height: 50px;
            text-align: center;
            font-size: 30px;
            font-weight: 700;
            position: absolute;
            color: #fff;
            display: none;
        }
.man-nav .navigation a.man-act{color:#000;font-weight: 600;}


/*好友弹窗*/
#friend{width:100%;height:100%;position: absolute;top:0;left:0;background:rgba(0,0,0,.6);z-index: 999999;display:none}
#friend .friend-box{width:100%;height:100%;position: relative;}
#friend .friend-box .content{width:600px;height:400px;position: absolute;top:50%;left:50%;background:#fff;transform: translate(-50%,-50%);}
#friend .friend-box .content .title{width:100%;height:50px;box-sizing: border-box;border-bottom: 1px solid #eee;}
#friend .friend-box .content .title span{display: inline-block;float:left;line-height: 50px;text-indent: 10px;font-weight: 600;font-size: 16px;}
#friend .friend-box .content .title i{display: inline-block;float:right;width:15px;height:15px;background:url(../image/delete_03.png) no-repeat;background-size: 100% 100%;margin:19px}
#friend .friend-box .content .title .go-back{display: none;text-indent: 40px;background:url(../image/go-back.png) no-repeat center left;height:50px;float:left;margin-left:10px;font-size: 17px;line-height: 50px;}





#friend .friend-box .content .friend-con{width:100%;height:350px;clear: both;}
#friend .friend-box .content .friend-con .box-one{width:100%;overflow: hidden;display: block;}
#friend .friend-box .content .friend-con .box-one .look{width:80%;height:30px;margin:30px auto 20px;font-size: 15px;line-height: 30px;}
#friend .friend-box .content .friend-con .box-one .look i{color:#055cfc;font-weight: 600;}
#friend .friend-box .content .friend-con .box-one .look span{margin:0 5px 0}
#friend .friend-box .content .friend-con .box-one .look input{box-sizing: border-box;height:30px;width:200px;text-indent: 10px;margin-right:10px}
#friend .friend-box .content .friend-con .box-one .look button{box-sizing: border-box;height:30px;width:60px;text-align: center;color:#fff;background:#055cfc;outline: none;border:none}

#friend .friend-box .content .friend-con .box-one .look-end{width:90%;overflow: hidden;margin:0 auto 0}
#friend .friend-box .content .friend-con .box-one .look-end .looktext{width:100%;height:30px;line-height: 30px;color:#055cfc;margin:20px auto 20px;font-size: 16px;}
#friend .friend-box .content .friend-con .box-one .look-end .look-pson{width:100%;height:60px;overflow: hidden;margin:10px auto 10px}
#friend .friend-box .content .friend-con .box-one .look-end .look-pson img{width:60px;height:60px;float:left;border-radius: 50%;margin-right:10px}
#friend .friend-box .content .friend-con .box-one .look-end .look-pson p{color:#919191}
#friend .friend-box .content .friend-con .box-one .look-end .look-pson span{display: inline-block;background:#055cfc;color:#fff;padding:0 3px;border-radius: 3px;}

#friend .friend-box .content .friend-con .box-tow{width:100%;overflow: hidden;display: none;}
#friend .friend-box .content .friend-con .box-tow .sub-peson{width:200px;height:60px;margin:20px auto 20px}
#friend .friend-box .content .friend-con .box-tow .sub-peson img{float:left;width:60px;height:60px;border-radius:50%;margin-right:10px;vertical-align: middle;}
#friend .friend-box .content .friend-con .box-tow .sub-peson .text{vertical-align: middle;box-sizing: border-box;padding-top:10px;}
#friend .friend-box .content .friend-con .box-tow .sub-peson .text p{color:#a2a2a2}

#friend .friend-box .content .friend-con .box-tow .send{width:80%;height:30px;margin:30px auto 20px;font-size: 15px;line-height: 30px;}
#friend .friend-box .content .friend-con .box-tow .send i{color:#055cfc;font-weight: 600;}
#friend .friend-box .content .friend-con .box-tow .send span{margin:0 5px 0}
#friend .friend-box .content .friend-con .box-tow .send input{box-sizing: border-box;height:30px;width:250px;text-indent: 10px;margin-right:10px}
#friend .friend-box .content .friend-con .box-tow .send button{box-sizing: border-box;height:30px;width:60px;text-align: center;color:#fff;background:#055cfc;outline: none;border:none}

/*成功的弹窗*/
#win{width:100%;height:100%;position: absolute;top:0;left;0;background:rgba(0,0,0,.6);z-index: 999999;display: none;}
.anima{animation: alls 0.5s ease-in-out;}
#win .win-box{width:100%;height:100%;position: relative;}
#win .win-box .com{width:250px;height:100px;background:#fff;position: absolute;top:50%;left:50%;transform: translate(-50%,-50%);}
#win .win-box .com img{width:40px;height:40px;float:left;margin:30px 100px;}
#win .win-box .com i{display: inline-block;width:12px;height:12px;position: absolute;top:10px;right:10px;background:url(../image/delete_03.png) no-repeat;background-size: 100% 100%;float:right}


@keyframes alls{
	0%{transform: scale(0);}
	100%{transform: scale(1);}
	/*100%{transform: scale(0);}*/
}