/*瀑布流*/
 .container { position: relative;margin: 0 auto;}
.container .box {float: left;-webkit-transition: left .3s ease-in-out, top .3s ease-in-out;background-color: #fff;margin: 10px 10px;border-radius: 5px;overflow: hidden;width:255px;}
.container .box:hover{box-shadow:0 5px 5px #999;}
.container .box .boximg{position: relative;}
.container .box .boximg i{display: block;width:50px;height:30px;background:#055cfc;text-align: center;line-height: 30px;color:#fff;font-size: 15px;position: absolute;top:13px;right:13px;border-radius: 5px;opacity: 0;}
.container .box .boximg .col-tow{display: inline-block;width:50px;height:30px;background:#055cfc;text-align: center;line-height: 30px;color:#fff;font-size: 15px;position: absolute;top:13px;right:13px;border-radius: 5px;opacity: 0;}
.container .box a{display: block;}
.container .box .boximg .rate{opacity: 0;display: inline-block;width:50px;height:30px;background:#d43636;text-align: center;line-height: 30px;font-size: 15px;position: absolute;top:10px;right:10px;color:#fff;border-radius: 5px;}
.container .box .boximg .rate1{opacity: 0;display: inline-block;width:50px;height:30px;background:#909090;text-align: center;line-height: 30px;font-size: 15px;position: absolute;top:10px;right:10px;color:#fff;border-radius: 5px;}
.container .box a:hover .boximg>img:hover{opacity: 0.8;}
.container .box a:hover .rate{opacity: 1;}
.container .box a:hover .rate1{opacity: 1;}
.container .box a:hover i{opacity: 1;}

.container .box .boximg{position: relative;}

.container .box .boximg .img-out{position: relative;overflow: hidden;}

.container .box .boximg .img-out .load{width:20px;height:20px;background:url(../image/aload.png) no-repeat center center;background-size: 20px 20px;animation: rota 5s linear;position: absolute;top:50%;left:50%;transform: translate(-50%,-50%);}


.container .box .boximg p{font-size:15px;color:#000;text-indent:10px;}
.container .box .boximg em{width:60px;height:20px;display: inline-block;line-height: 20px;text-align: center;background: #b9b9b9;font-size: 15px;color:#fff;border-radius: 5px;margin:20px 10px 0}
.container .box img {width: 255px;padding-bottom: 10px;position: relative;opacity: 0;}
.container .box .behavior{width:90%;margin:30px auto 0;border-top:1px solid #eee}
.container .box .behavior .peson{display: inline-block;overflow: hidden;}
.container .box .behavior .peson img{width:40px;height:40px;border-radius: 50%;display: inline-block;float:left;margin-top:10px}
.container .box .peson span{display: inline-block;line-height: 40px;float:left;margin-top:10px;margin-left:10px}

.container .box .behavior .praise{display: inline-block;overflow: hidden;float:right}
.container .box .behavior .praise span{display: inline-block;width:20px;height:20px;background:url(../image/zan.png) no-repeat;float:left;margin-top:15px;margin-right:10px;background-size: 100% 100%;}
.container .box .behavior .praise .yizan{display: inline-block;width:20px;height:20px;background:url(../image/yizan.png) no-repeat;float:left;margin-top:15px;margin-right:10px;background-size: 100% 100%;}
.container .box .behavior .praise strong{display: inline-block;width:20px;height:20px;text-align: center;line-height: 20px;float:left;margin-top:15px;margin-right:0px;color:#989898}
.container .box .behavior .praise em{display: inline-block;height:20px;font-size:15px;line-height: 20px;color:#000;float:left;margin-top: 15px;opacity: 1;}
.container .box .behavior .praise i{display: inline-block;height:20px;font-size:15px;line-height: 20px;color:#000;float:left;margin-top: 15px;opacity: 1;}


