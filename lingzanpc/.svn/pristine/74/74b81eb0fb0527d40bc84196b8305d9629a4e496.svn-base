.add-btn{width: 100%;background-color: #ff7d3c;height: 50px;
            line-height: 50px;text-align: center;font-size: 30px;font-weight: 700;
            position: absolute;color: #fff; display: none;
            }
            
@keyframes rota{
	from{transform: rotate(360deg);transform-origin: center;}
	to{transform: rotate(0deg);transform-origin: center;opacity: 0;}
}
