/*评论留言*/
.product .right .message{overflow: hidden;}
.product .right .message .leave{height:40px;line-height: 40px;color:#000;font-size: 14px;text-indent: 10px;margin-top:10px;overflow: hidden;}
.product .right .message .leave i{color:#989898;margin-left:10px}
.product .right .message .leave a{float:right;font-size: 13px;color:#055cfc;margin:10px 10px 0;text-decoration: underline;}
.product .right .message .ping{overflow: hidden;display: flex;}
.product .right .message .ping input[type="text"]{height:40px;flex: 1;display: inline-block;margin:0 10px 0 10px}
.product .right .message .ping button{display: inline-block;height:40px;background:red;width:70px;display: inline;outline: none;margin-right:50px;border: none;background:url(../image/fasong_03.png) no-repeat;background-size: 100% 100%;}
.product .right .message .person{overflow: hidden;}
.product .right .message .person ul{overflow: hidden;}
.product .right .message .person ul li{width:95%;border-bottom: 1px solid #eee;margin:0 auto 0;display: flex;padding:5px 0}
.product .right .message .person ul li .pson{display: inline-block;height:100%;float:left;overflow: hidden;}
.product .right .message .person ul li .pson img{width:30px;height:30px;float:left;border-radius: 50%;margin-right:20px}
.product .right .message .person ul li .text{display: inline-block;float:right;flex: 1;font-size: 12px;line-height: 25px;}
.product .right .message .person ul li .text span{color:#989898}
.product .right .message .person strong{cursor: pointer;display: inline-block;width:80px;height:20px;line-height: 20px;;color:#000;float:right;text-align: center;margin-right:-3px;margin-top:10px;margin-bottom:20px;text-decoration: underline;font-size: 13px;font-weight: 400;}
/*这里是清单*/
.detail{width:100%;background:#ebebeb;overflow: hidden;}
.detail .detail-box{width:90%;margin:20px 0 0 110px;overflow: hidden;}
.detail .detail-box h2{font-size:20px;text-align: center;line-height: 30px;margin:10px auto 10px}

.detail .detail-box .detail-title{width:1200px;height:70px;box-sizing: border-box;border-bottom:1px solid #989898;position: relative;}
.detail .detail-box .detail-title:after{content: '';display: block;clear: both;}
.detail .detail-box .detail-title select{display: inline-block;width:90px;border: none;outline: none;height:30px;font-size: 16px;margin-left:20px;background: #ebebeb;}
/*.detail .detail-box .detail-title:after{content: '';display: block;clear: ;}*/
.detail .detail-box .detail-title .pro{display: inline-block;}
.detail .detail-box .detail-title .pro h1{font-size: 24px;line-height: 80px;display: inline-block;font-weight: 400;position: absolute;left:50%;transform: translateX(-50%);}
.detail .detail-box .detail-title .pro span{display: inline-block;line-height: 80px;font-size: 16px;}
.detail .detail-box .detail-title #right-edit{display: inline-block;float:right;display: block;margin-left:5px}
.detail .detail-box .detail-title .change{display: inline-block;float:left;height:40px;overflow: hidden;margin-top:20px;margin-right:0px}
.detail .detail-box .detail-title .change .turn{display: inline-block;height:40px;line-height:40px;width:130px;border-radius:5px;text-indent: 30px;background:#055cfc;color:#fff;font-size: 13px;margin-right:20px}
.detail .detail-box .detail-title .change .turn .turn-box{display: block;background:url(../image/bai.png) no-repeat 10px center;}
/*这里是搜索*/
.detail .detail-box .detail-title .detail-sear{font-size: 16px;width:150px;height:40px;position: absolute;left:45%;/*transform: translateX(-50%);*/top:15px;border-bottom:1px solid #055cfc;box-sizing: border-box;overflow: hidden;}
