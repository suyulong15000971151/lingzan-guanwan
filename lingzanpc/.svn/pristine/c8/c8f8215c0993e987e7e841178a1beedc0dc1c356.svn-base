/*这是项目标题*/
.sub-nav{width:100%;background:#fff;overflow: hidden;box-sizing: border-box;border-bottom:1px solid #d0d0d0;margin:0 auto 0}
.sub-box{width:1200px;margin:0 auto 0;position: relative;}
.sub-box-title{left:30%}

.sub-box .pro-explain{width:600px;position: relative;line-height: 20px;height:40px;overflow: hidden;text-overflow: ellipsis;display: inline-block;float:left;margin:20px 10px 20px;margin-left:80px;font-size: 16px;}
.sub-box .pro-explain em{display: inline-block;line-height: 20px;position: absolute;bottom:0;right:5px;color:#055cfc;cursor: pointer;}
.sub-box:after{content: '';display: block;clear: both;}
.sub-nav h1{line-height: 80px;font-size:24px;display: inline-block;float:left;height:80px;text-indent: 10px;float:left;margin-left:200px}
.sub-nav strong{line-height: 80px;font-size: 20px;display: inline-block;float:left;margin-left:5px;font-weight: 400;font-weight: 400;}
.sub-nav select{text-indent: 10px;display: inline-block;float:left;line-height: 35px;font-size: 16px;color:#000;margin:0 0px 0;height:35px;box-sizing: border-box;margin-top:23px;font-weight: 400;width:170px;float:left}
.sub-nav select option{text-indent: 10px;}

.sub-nav .caption{text-indent: 10px;display: inline-block;float:left;line-height: 35px;font-size: 20px;color:#000;margin:0 0px 0;height:35px;box-sizing: border-box;margin-top:23px;font-weight: 600;width:170px;float:left;white-space: nowrap;overflow: hidden;}
.sub-nav i{display: inline-block;float:right;line-height: 80px;font-size: 18px;color:#000;font-weight: 600;text-indent: 40px;background:url(../image/back-history.png) no-repeat center left;background-size: 20px 20px;cursor: pointer;display: inline-block;width:30px;height:30px;margin-top:25px;margin-right:12px}
.sub-nav span{cursor: pointer;display: inline-block;box-sizing: border-box;width:40px;line-height: 40px;text-indent: 32px;height:40px;float:right;margin:20px;background:url(../image/sh.png) no-repeat 10px center;background-size: 20px 22px;margin-right:32px}
.sub-nav .files{width:100px;height:40px;box-sizing: border-box;display: inline-block;float:right;margin:20px 10px}
.sub-nav .files a{display: inline-block;width:30px;height:40px}
.sub-nav .files p{text-align: center;line-height: 40px;display: inline-block;font-size: 12px;width:40px}
/*.sub-nav .files a.add{display: inline-block;float:left;line-height: 40px;font-size: 22px;font-weight: 600;text-align: center;color:#000;}*/
.sub-nav .files a.load{display: inline-block;float:right;}
.sub-nav .files a.load img{display: block;margin:5px auto 0;width:30px;height:30px}
.sub-nav .files p{box-sizing: border-box;position: relative;}
.sub-nav .files p em{display: block;cursor: pointer;width:20px;height:20px;border-radius: 31%;color:#055cfc;text-align: center;line-height: 20px;position: absolute;left:50%;top:40%;transform: translate(-50%,-50%);font-size: 13px;}
.sub-nav .files p a{background:url(../image/wendang.png) no-repeat center center;background-size:30px 30px;}
.sub-nav .files a.add img{display: block;margin:5px auto 0;width:30px;height:30px}





.product{margin:0 auto 0;clear: both;width:1200px;position: relative;z-index: 9;}
.product .left{width:871.5px;display: inline-block;float:left;position:relative;overflow: hidden;}
.product .left .item{width:220px;overflow: hidden;display: inline-block;float:left;margin:20px 23px 0 0px;}
.product .left .item ul{overflow-y: scroll;width:310px;height:440px}
.product .left .item ul li{width:103px;overflow: hidden;height:103px;margin-bottom:9px;float:left;box-sizing: border-box;border:1px solid #ccc;margin-right:10px;position: relative;}
.product .left .item ul li.increase{background:url(../image/crease.png) no-repeat center}
.product .left .item ul li.increase input{cursor: pointer;display: block;height:100%;opacity: 0;}
.product .left .item ul li .bg{display: block;width:100%;height:100%}
.product .left .item ul li i{width:15px;height:15px;display: none;position: absolute;right:0;top:0;background:url(../image/xx.png) no-repeat;background-size: 100% 100%;}
.product .left .item ul li:hover i{display: block;}
.product .left .item button{outline: none;border:none;background:#47a447;color:#fff;width:70px;height:20px;border-radius: 2px;line-height: 20px;text-align: center;}
.product .left .item ul li em{display: block;position: absolute;bottom:0;left:0;width:0;background:rgb(100,149,198);height:10px}
.product .left .item ul li em.progess{animation: pro 1s linear;}

.product .left .item ul li img{width:100%;height:100%}
.product .left .show{position: relative;width:580px;display: inline-block;float:left;margin-top:20px;margin-bottom:50px;margin-left:10px;box-sizing: border-box;height:435px;border:1px solid #eee;}
.product .left .show a{white-space: nowrap;overflow: hidden;display: inline-block;text-align: center;line-height: 30px;font-size: 15px;position: absolute;bottom:-30px;left:50%;transform: translateX(-50%);height:30px;padding:0 10px}
.product .left .show .trun{display:none;width:70px;height:30px;background:#055cfc;color:#fff;border-radius: 5px;text-align: center;line-height: 30px;position: absolute;right:20px;top:20px;height:30px;white-space: nowrap;}
.product .left .show:hover .trun{display: block;}
.product .left .show img{display: block;margin:0 auto 0}
.product .left a.arror-l{width:20px;height:35px;background:url(../image/arrorl.png) no-repeat;background-size: 100% 100%;position: absolute;left:10px;top:300px}
.product .left a.arror-r{width:20px;height:35px;background:url(../image/arrorr.png) no-repeat;background-size: 100% 100%;position: absolute;left:920px;top:300px}
.product .right{width:300px;display: inline-block;float:left;overflow: hidden;margin-top:20px;height:435px;box-sizing: border-box;border:1px solid #eee;}
.product .right .title{box-sizing: border-box;padding-right:10px;border-bottom:1px solid #eee;overflow: hidden;height:80px}
.product .right .title h1{margin-left:10px;font-size: 18px;color:#000;line-height: 30px;white-space: nowrap;overflow: hidden;text-overflow: ellipsis;display: inline-block;padding-top:0px;font-weight: 400;font-size:14px;margin-bottom:40px}
.product .right .title span{display: inline-block;float:right;height:100px;line-height: 100px;text-indent: 30px;font-size:18px;background:url(../image/share.png) no-repeat center left;margin-right:30px;font-weight: 500;}
.product .right .title p{font-size:13px;line-height: 20px;box-sizing: border-box;padding-left:10px;margin-bottom: 5px;height:70px;overflow: hidden;width:220px;display: inline-block;padding-top:10px}
.product .right .title img{width:30px;height:30px;border-radius: 50%;display: inline-block;margin-left:10px;vertical-align: middle;}
.product .right .title em{line-height: 30px;text-indent: 5px;display: inline-block;width:75px;white-space:nowrap;overflow: hidden;vertical-align: middle;}
.product .right .title .pso{display: inline-block;overflow: hidden;float:right;margin-top:10px}
/*加关注*/
.product .right .follow{overflow: hidden;border-bottom:1px solid #eee;height:50px;display: none;}
.product .right .follow .follow-l{overflow: hidden;display: inline-block;float:left}
.product .right .follow .follow-l img{display: inline-block;width:40px;height:40px;border-radius: 50%;float:left;margin:5px 10px 0}
.product .right .follow .follow-l span{line-height: 50px;display: inline-block;font-size: 13px;}
.product .right .follow .follow-r{overflow: hidden;display: inline-block;float:right;line-height: 80px;font-size: 16px;margin-right:50px}

/*这里是邀请项目成员*/
.members{width:100%;padding-left:10px;display: block;clear: both;position: relative;height:140px;margin-bottom:20px;}
.members .member .arror{display: inline-block;width:40px;height:40px;position: absolute;top:5px;background:#ccc;border-radius: 50%;line-height: 35px;text-align: center;font-size: 30px;font-weight: 600;color:#fff;cursor: pointer;}
.members .member .arror-left{left:-80px;}
.members .member .arror-right{right:-80px;}
.members .member .arror-left a{display: block;width:100%;height:100%;background:url(../image/upper.png) no-repeat center center;background-size: 20px 20px;}
.members .member .arror-right a{display: block;width:100%;height:100%;background:url(../image/down.png) no-repeat center center;background-size: 20px 20px;}


.members:after{content: "";display: block;clear: both;}
.members .member{display: block;width:500px;margin:0 auto 0;position: relative;}
.members .member h4{font-size: 14px;line-height: 25px;margin:10px 0;text-align: center;}
.members .member .member-add{margin-top:10px;position: relative;}

.members .member .member-add #power{width:390px;height:470px;background:#fff;box-shadow: 0 0 5px #000;position: absolute;top:80px;left:50%;transform: translateX(-50%);display: none;}
.members .member .member-add #power .title{width:100%;height:40px;box-sizing: border-box;border-bottom:1px solid #dadfdf;line-height: 40px;}
.members .member .member-add #power .title span{font-size: 12px;text-indent: 10px;}
.members .member .member-add #power .title i{display: inline-block;width:10px;height:10px;float:right;margin:15px 10px 0;background:url(../image/delete_03.png) no-repeat;background-size: 100% 100%;}
.members .member .member-add #power .power_nav{width:100%;height:50px;line-height: 50px;text-align: center;font-size: 13px;border-bottom:1px solid #dadfdf;box-sizing: border-box;}
.members .member .member-add #power .power_nav a{display: inline-block;height:50px;margin:0 20px;color:#999;position: relative;}
.members .member .member-add #power .power_nav a.tv{color:#000}
.members .member .member-add #power .power_nav a em{position: absolute;width:20px;height:2px;background:#333;top:40px;left:50%;transform: translateX(-50%);}
.members .member .member-add #power .pdcu{width:100%;box-sizing: border-box;border-bottom:1px solid #dadfdf;}
.members .member .member-add #power .pdcu .col{display: inline-block;float:left;margin-left:20px}
.members .member .member-add #power .pdcu .col .sum_check{cursor: pointer;display: block;width:50px;height:20px;line-height: 20px;font-size: 12px;margin:34px auto 0;color:#999}
.members .member .member-add #power .ord .col .sum_check{margin-top:20px}
.members .member .member-add #power .peo .col .sum_check{margin-top:7px}

.members .member .member-add #power .pdcu .col .sum_check i{vertical-align: middle;display: inline-block;width:15px;height:15px;box-sizing: border-box;border:1px solid #acacac;margin-right:5px}
.members .member .member-add #power .pdcu .col .sum_check i.ck{background:url(../image/gou.png) no-repeat;background-size:100% 100%;}
.members .member .member-add #power .pdcu .col-sub{margin-left:80px;color:#999;margin-right:20px;padding:8px 0}
.members .member .member-add #power .pdcu .col-sub:after{content: '';display: block;clear: both;}
.members .member .member-add #power .pdcu .col-sub .sub_check{cursor: pointer;display: inline-block;width:130px;height:20px;line-height: 20px;float: left;margin-left:15px;font-size: 12px;margin-bottom:5px;position: relative;}
.members .member .member-add #power .pdcu .col-sub .sub_check em{display: none;width:20px;height:20px;position: absolute;right: 30px;top:0;background:url(../image/bmg.png) no-repeat center center;}
.members .member .member-add #power .pdcu .col-sub .sub_check .explain{width:100px;padding:2px 5px;background:#fff;box-shadow: 0 0 5px #000;position: absolute;top:-25px;left:0;display: none;}

.members .member .member-add #power .pdcu .col-sub .sub_check:hover em{display: inline-block;}
.members .member .member-add #power .pdcu .col-sub .sub_check:hover .explain{display: block;}
.members .member .member-add #power .pdcu .col-sub .sub_check i{vertical-align: middle;display: inline-block;width:15px;height:15px;box-sizing: border-box;border:1px solid #acacac;margin-right:5px}
.members .member .member-add #power .pdcu .col-sub .sub_check i.ck{background:url(../image/gou.png) no-repeat;background-size:100% 100%;}
.members .member .member-add #power .confirm{width:100%;overflow: hidden;text-align: center;box-sizing: border-box;padding:20px 0}
.members .member .member-add #power .confirm a{display: inline-block;width:50px;height: 25px;line-height: 25px;text-align: center;margin:0 20px;font-size: 12px;border-radius: 5px;}

.members .member .member-add img{width:55px;height:55px;display: inline-block;vertical-align: top;margin-right:5px}
.members .member .member-add span{display: inline-block;line-height: 40px;color:#666;font-size: 13px;}
.members .member .member-add .peon-item{display: inline-block;width:55px;height:55px;margin:0 5px 0;position:relative}
.members .member .member-add .peon-item img{border-radius: 50%;}
.members .member .member-add .peon-item i{display: block;line-height: 25px;text-align: center;margin-top:5px;font-size: 12px;}
.members .member .member-add .peon-item .people-id{width:270px;height:200px;background:#fff;box-shadow: 0 0 5px rgba(0,0,0,.7);box-sizing: border-box;padding-top:10px;position:absolute;top:50%;left:50%;z-index: 9999;display: none;}
.members .member .member-add .peon-item .people-id .rol{width:250px;height:175px;margin:0 auto 0}
.members .member .member-add .peon-item .people-id .rol .left{width:100px;height:100%;display: inline-block;float:left;box-sizing: border-box;border-right:1px solid #eee}
.members .member .member-add .peon-item .people-id .rol .left img{display: block;margin:40px auto 10px;width:60px;height:60px}
.members .member .member-add .peon-item .people-id .rol .left p{text-align: center;height:30px;line-height: 30px;font-size: 16px;}

.members .member .member-add .peon-item .people-id .rol .right{width:140px;height:100%;display: inline-block;float:left;font-size: 12px;border:none;position: relative;}
.members .member .member-add .peon-item .people-id .rol .right span{display: inline-block;width:80px;white-space: nowrap;font-size: 12px;height:20px;line-height: 20px;vertical-align: middle;}
.members .member .member-add .peon-item .people-id .rol .right input.ed{display: inline-block;width:70px;height:20px;text-indent: 5px;background:#eee;border:1px solid #ccc;outline: none;}
.members .member .member-add .peon-item .people-id .rol .right input{display: inline-block;width:70px;height:25px;text-indent: 5px;background:#fff;border:none;outline: none;}
.members .member .member-add .peon-item .people-id .rol .right label{margin-left:5px;margin-right:5px}
.members .member .member-add .peon-item .people-id .rol .right strong{cursor: pointer;display: inline-block;width:20px;height:20px;position: absolute;top:0px;right:10px;background:url(../image/powers.png) no-repeat;background-size: 20px 20px;}

