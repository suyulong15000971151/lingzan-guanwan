body{background:#f6f6f6}
/*这是banner区域*/
.manpeople{width:100%;height:200px;border-bottom: 1px solid #eee;background:#fff}
.manpeople .man-box{width:100%;height:150px;margin:0 auto 0;overflow: hidden;box-sizing: border-box;}
.manpeople .man-box i{display: inline-block;height:30px;width:100px;font-size: 18px;line-height: 30px;text-indent: 40px;background:url(../image/back.png) no-repeat left center;background-size: 27px 22px;float:right;margin:20px 30px}
.manpeople .man-box h1{font-size: 30px;line-height: 40px;text-align: center;margin:50px 0 10px 0;clear: both;}
.manpeople .man-box p{font-size: 18px;text-align: center;line-height: 30px;}


/*这里是下面展示的内容*/
/*{width:100%;overflow: hidden;background:#f6f6f6;padding:30px 0 0;width:95%;margin:0 auto 0;padding-bottom:40px}*/
 .man-item{width:250px;height:320px;background:#fff;display: inline-block;float:left;border:1px solid #eae9e9;position: relative;padding-bottom:10px;height:330px}
 .man-item p{font-size:20px;line-height: 40px;text-indent:100px;margin-top:130px;height:40px;background:url(../image/creat.png) no-repeat 50px center}
 .man-item .edits{margin-top:10px;width:110px;display: inline-block;background:#f4f4f4;height:30px;font-size:14px;line-height: 30px;text-align: center;border:1px solid #dcdcdc;box-sizing: border-box;}
 .man-item .edidele{width:110px;height:30px;display: inline-block;margin-left:2px;border:1px solid #dcdcdc;font-size: 14px;text-align: center;line-height: 30px;background:#f4f4f4}
 .man-item .item-box{width:230px;overflow: hidden;display: block;margin:10px auto 0}
 .man-item .item-box h4{font-size:14px;line-height: 25px;margin-bottom: 5px;}
 .man-item .item-box .big-img{width:100%;height:150px;overflow: hidden;margin-bottom:5px}
 .man-item .item-box .big-img img{width:100%;height:100%;}
 .man-item .item-box .small{width:260px;overflow: hidden;}
 .man-item .item-box .small img{display: inline-block;float:left;width:70px;height:70px;margin-right:10px}
 .man-item .state{position: absolute;right:-100px;top:10px;width:100%;height:30px;background: #055cfc;text-align: center;color:#fff;line-height: 30px;transform: rotate(45deg);transform-origin:center ;}
 .man-item .production{background:#000;}
 .man-item .win{background:#ababab;}
.add-btn{
            width: 100%;
            background-color: #ff7d3c;
            height: 50px;
            text-align: center;
            font-size: 30px;
            font-weight: 700;
            position: absolute;
            color: #fff;
            display: none;
        }
