/*瀑布流*/
 .containers { position: relative;margin: 20px auto 0;}
.containers .box{float: left;-webkit-transition: left .3s ease-in-out, top .3s ease-in-out;background-color: #fff;margin: 10px 10px;border-radius: 5px;overflow: hidden;position: relative;width:250px;box-sizing: border-box;}
.containers .box .delete{border-radius: 5px;display: inline-block;width:40px;height:25px;text-align: center;font-size: 14px;line-height: 25px;background:#f6f6f6;color:#000;position: absolute;top:10px;left:10px;z-index: 1000;opacity: 0;}
.containers .box .edit{border-radius: 5px;display: inline-block;width:40px;height:25px;text-align: center;font-size: 14px;line-height: 25px;background:#055cfc;color:#fff;position: absolute;top:10px;right:10px;z-index: 1000;opacity: 0;}
.containers .box:hover .edit{opacity: 1;}
.containers .box:hover .delete{opacity: 1;}

.containers .box:hover{box-shadow:0 5px 5px #999;}
.containers .box .boximg{position: relative;}
.containers .box .boximg i{display: inline-block;width:50px;height:30px;background:#055cfc;text-align: center;line-height: 30px;color:#fff;font-size: 15px;position: absolute;top:13px;right:13px;border-radius: 5px;opacity: 0;}
.containers .box .boximg span{display: inline-block;font-size: 16px;color:#055cfc;line-height: 20px;text-indent: 10px;margin:10px 0 0}
.containers .box .boximg p{font-size:15px;color:#000;text-indent:10px;}
.containers .box .boximg em{width:60px;height:20px;display: inline-block;line-height: 20px;text-align: center;background: #b9b9b9;font-size: 15px;color:#fff;border-radius: 5px;margin:20px 10px 0}
.containers .box img {width: 255px;padding-bottom: 10px;}
.containers .box a:hover .boximg>img:hover{opacity: 0.8;}
.containers .box .behavior{width:90%;margin:30px auto 0;border-top:1px solid #eee}
.containers .box .behavior .peson{display: inline-block;overflow: hidden;}
.containers .box .behavior .peson img{width:40px;height:40px;border-radius: 50%;display: inline-block;float:left;margin-top:10px}
.containers .box .peson span{display: inline-block;line-height: 40px;float:left;margin-top:10px;margin-left:10px}

.containers .box .behavior .praise{display: inline-block;overflow: hidden;float:right}
.containers .box .behavior .praise span{display: inline-block;width:20px;height:20px;background:url(../image/zan.png) no-repeat;float:left;margin-top:15px;margin-right:10px}
.containers .box .behavior .praise strong{display: inline-block;width:20px;height:20px;text-align: center;line-height: 20px;float:left;margin-top:15px;margin-right:0px;color:#989898}
.containers .box .behavior .praise em{display: inline-block;height:20px;font-size:15px;line-height: 20px;color:#000;float:left;margin-top: 15px;}
.containers .box a{display: block;}


.containers .box a{display: block;position: relative;}
