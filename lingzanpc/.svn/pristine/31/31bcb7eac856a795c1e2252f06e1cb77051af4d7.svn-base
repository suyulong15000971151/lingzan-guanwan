body{background:#f6f6f6}
/*这是banner区域*/
.manpeople{width:100%;height:200px;border-bottom: 1px solid #eee;background:#fff}
.manpeople .man-box{width:300px;height:150px;margin:0 auto 0;overflow: hidden;display: flex;box-sizing: border-box;padding-top:30px}
.manpeople .man-box .potato{display: inline-block;width:100px;height:100%;float:left;margin-right:10px}
.manpeople .man-box .potato img{width:100px;height:100px;border-radius: 50%;}
.manpeople .man-box .explain{display: inline-block;float:left;flex: 1;}
.manpeople .man-box .explain i{display: inline-block;width:20px;height:20px;background:url(../image/vip.png) no-repeat;background-size:100% 100%;vertical-align: middle;margin-right:5px}
.manpeople .man-box .explain h2{font-size: 23px;margin:10px 0 5px}
.manpeople .man-box .explain p{font-size: 14px;color:#a2a2a2;margin-bottom:5px}
.manpeople .man-box .explain strong{}
/*这里是个人导航也*/
.man-nav{width:100%;height:100px;background:#fff;overflow: hidden;}
.man-nav .navigation{overflow: hidden;width:100%;height:100%;text-align: center;}
.man-nav .navigation a{display: inline-block;height:100%;font-size: 18px;line-height: 25px;color:#919191;padding:0 40px}
.man-nav .navigation a span{margin-top:20px;display: inline-block;}
/*这里是下面展示的内容*/
.man-show{width:100%;overflow: hidden;background:#f6f6f6;padding:30px 0 0;width:99%;margin:0 auto 0;padding-bottom:40px}
.man-show .man-showsub{width:105%;overflow: hidden;margin: 0 auto 0;}
.man-show .man-item{width:250px;box-sizing: border-box;height:275px;background:#fff;display: inline-block;float:left;border:1px solid #eae9e9;margin-right:20px;overflow: hidden;position: relative;margin-bottom:27px}
.man-show .man-item p{font-size:20px;line-height: 40px;text-indent:100px;margin-top:130px;height:40px;background:url(../image/creat.png) no-repeat 50px center}
.man-show .man-item .edit{margin-top:10px;width:100%;height:30px;font-size:14px;line-height: 30px;text-align: center;border:1px solid #dcdcdc;box-sizing: border-box;}

.man-show .man-item .item-box{width:230px;overflow: hidden;display: block;margin:10px auto 0}
.man-show .man-item .item-box h4{font-size:14px;line-height: 25px;margin-bottom: 5px;}
.man-show .man-item .item-box .big-img{width:100%;height:150px;overflow: hidden;margin-bottom:5px}
.man-show .man-item .item-box .big-img img{width:100%;height:100%;}
.man-show .man-item .item-box .small{width:260px;overflow: hidden;height:70px}
.man-show .man-item .item-box .small img{display: inline-block;float:left;width:70px;height:70px;margin-right:10px}
.man-show .man-item .state{position: absolute;right:-100px;top:10px;width:100%;height:30px;background: #055cfc;text-align: center;color:#fff;line-height: 30px;transform: rotate(45deg);transform-origin:center ;}
.man-show .man-item .production{background:#000;}
.man-show .man-item .win{background:#ababab;}
.add-btn{
            width: 100%;
            background-color: #ff7d3c;
            height: 50px;
            text-align: center;
            font-size: 30px;
            font-weight: 700;
            position: absolute;
            color: #fff;
            display: none;
        }
.man-nav .navigation a.man-act{color:#000;font-weight: 600;}
/*这是创建材思库弹窗*/
#pick .pick-box .pick-content .pick-show .pick-left .img-load{width:180px;height:180px;margin:5px auto 5px;overflow: hidden;background:#e8e8e8;position: relative;}
#pick .pick-box .pick-content .pick-show .pick-left .img-load input{width:100%;height:40px;outline: none;border:none;position: absolute;display: block;top:50px;opacity: 0;}

#pick .pick-box .pick-content .pick-show .pick-left .img-load p{width:100px;color:#fff;text-indent: 0;height:40px;background:#055cfc;line-height: 40px;text-align: center;margin:50px auto 0;font-size: 15px;font-weight: 400;}
#pick .pick-box .pick-content .pick-show .pick-left .title-item{width:180px;height:40px;line-height: 40px;border:1px solid #919191;box-sizing: border-box;position: relative;margin:0 auto 10px} 
#pick .pick-box .pick-content .pick-show .pick-left .title-item input{width:150px;vertical-align: top;height:38px;outline: none;border:none;text-indent: 13px;box-sizing: border-box;}
#pick .pick-box .pick-content .pick-show .pick-left .title-item span{position: absolute;display: inline-block;height:40px;line-height: 40px;left:5px;color:#055cfc}
#pick .pick-box .pick-content .pick-show .pick-left .title-item .classif{width:100%;height:40px}
#pick .pick-box .pick-content .pick-show .pick-left .title-item i{display: inline-block;position: absolute;border-left:4px solid transparent;border-top:4px solid transparent;border-bottom:4px solid #055cfc;border-right:4px solid #055cfc;right:5px;bottom:5px}
#pick .pick-box .pick-content .pick-show .pick-left .title-item p{width:100%;font-weight: 400;height:40px;line-height: 40px;text-indent:13px;font-size: 14px;margin-top:0px;color:#6e6e6e}
#pick .pick-box .pick-content .pick-show .pick-left textarea{color:#6e6e6e;line-height: 25px;text-indent: 10px;}

#pick .pick-box .pick-content .pick-show .pick-left .title-item input{display: inline-block;position: absolute;opacity:0}
#pick .pick-box .pick-content .pick-show .pick-left .title-item .class-item{display: none;width:300px;z-index: 555;background:#fff;position: absolute;box-shadow: 0 0 5px rgba(0,0,0,.7);top:45px;left:0px;overflow: hidden;}
#pick .pick-box .pick-content .pick-show .pick-left .title-item .class-item a{display: inline-block;padding:0px 4px}
/*这里是编辑材思库弹窗*/
#edit{width:100%;height:100%;position:absolute;top:0;background:rgba(0,0,0,.7);z-index: 555555;display: none;}
#edit .edit-box{width:100%;height:100%;position:relative}
#edit .edit-box .edit-content{width:600px;height:500px;position: absolute;top:50%;left:50%;transform: translate(-50%,-50%);background:#fff}
#edit .edit-box .edit-content .title{width:100%;height:60px;box-sizing: border-box;border-bottom:1px solid #dadfdf;font-size: 18px;line-height: 60px;}
#edit .edit-box .edit-content .title span{color:#000;margin:0 5px 0 20px;display: inline-block;font-weight: 600;}
#edit .edit-box .edit-content .title em{color:#055cfc;display: inline-block;font-weight: 600;}
#edit .edit-box .edit-content .title i{display: inline-block;float:right;width:16px;height:16px;background:url(../image/delete_03.png) no-repeat;background-size: 100% 100%;margin:22px 20px}
#edit .edit-box .edit-content .edit-container{width:300px;margin:0 auto 0}
#edit .edit-box .edit-content .edit-container .edit-item{display: flex;width:100%;height:40px;margin:10px 0 0;font-size: 15px;line-height: 40px;}
#edit .edit-box .edit-content .edit-container .edit-item label{}
#edit .edit-box .edit-content .edit-container .edit-item .item-con{flex: 1;border:1px solid #989898;height:40px;box-sizing: border-box;position: relative;}
#edit .edit-box .edit-content .edit-container .edit-item .item-con span{position: absolute;left:5px;line-height: 40px;color:#055cfc;font-weight: 600;}
#edit .edit-box .edit-content .edit-container .edit-item .item-con .sort{width:100%;height:100%;color:#a2a2a2;text-indent: 15px;}
#edit .edit-box .edit-content .edit-container .edit-item .item-con i{position:absolute;right:5px;bottom:5px;border-bottom:4px solid #055cfc;border-right:5px solid #055cfc;border-left:4px solid transparent;border-top:4px solid transparent}
#edit .edit-box .edit-content .edit-container .edit-item input{width:100%;height:38px;box-sizing: border-box;outline: none;text-indent: 10px;border:none;vertical-align: top;}
#edit .edit-box .edit-content .edit-container .power .item-con{border:none}
#edit .edit-box .edit-content .edit-container .delete .item-con{border:none}
#edit .edit-box .edit-content .edit-container .power input[type="radio"]{width:15px;height:15px;vertical-align: middle;}
#edit .edit-box .edit-content .edit-container .delete .item-con em{display: inline-block;width:150px;height:40px;text-align:center;background:#e2e2e2;border:1px solid #999;box-sizing: border-box;}
#edit .edit-box .edit-content .edit-container button{width:200px;height:40px;text-align: center;line-height: 40px;color:#fff;font-weight: 600;background:#055cfc;outline: none;border:none;display: block;margin:30px auto 0;font-size: 16px;}
#edit .edit-box .edit-content .sele-edit{display: none;width:300px;position:absolute;overflow: hidden;background:#fff;box-shadow:0 0 5px rgba(0,0,0,.7);z-index: 999;}
#edit .edit-box .edit-content .sele-edit a{display: inline-block;padding:2px 3px}

/*获取创建材思库的名称值*/
#pick .pick-box .pick-content .pick-show .pick-right .select-box input{display: inline-block;position: absolute;opacity: 0;}

.imgBox{width:100%;height:100%;z-index: 9999;}
.imgBox .bg{width:100%;height:100%;}
#pick .pick-left .img-load i{display: inline-block;width:12px;height:12px;position: absolute;right:5px;top:5px;background:url(../image/delete_03.png) no-repeat;background-size: 100% 100%;}

@media only screen and (min-width: 1000px) and (max-width: 1400px) {
	.man-show{width:90%}
}