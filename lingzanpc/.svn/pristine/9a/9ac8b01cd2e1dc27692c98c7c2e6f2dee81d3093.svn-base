@charset "utf-8";
*{margin:0;padding: 0;}
a{text-decoration: none;outline:none;color:#000;
   -moz-outline-style:none;}
ul{list-style-type: none;}
i,em,address{font-style: normal;}
/*img{display: block;max-width: 100%;outline: none;border:none}*/
body{
	padding-top:80px;
	 -webkit-tap-highlight-color: rgba(0,0,0,0);
	background: #fff;;
	margin:0 auto 0;
    font-family: '微软雅黑';
	font-size: 14px;
	
}
a.active{color:#fff}
/*这是头部*/
header{height:80px;background:#000;padding:0 30px 0;position: fixed;top:0;width:100%;box-sizing: border-box;z-index: 9999;}
header .log{display: inline-block;overflow: hidden;float:left}
header .log img{width:200px;height:40px;margin-top:20px;float:left}
header .nav{display: inline-block;float:left;margin-left:80px}

header .nav a{padding:0 20px 0;font-size: 18px;color:#777;line-height: 80px;}
header .nav a.active{color:#fff;}
header .nav span{float:right;margin-top:30px;margin-left:15px;display: inline-block;width:20px;height:23px;background:url(../image/navsearch.png) no-repeat;background-size: 100% 100%;}

header .right{display: inline-block;float:right}
/*这里是头部签到*/
header .right .sign{width:70px;height:80px;padding:20px 0 20px;display: inline-block;float:left;box-sizing: border-box;margin-right:20px;position: relative;}
header .right .sign p{text-indent: 30px;color:#fff;line-height: 40px;font-size: 15px;height:40px;background:#055cfc;}
header .right .sign strong{display: inline-block;width:20px;height:30px;position: absolute;top:20px;left:10px;background:url(../image/sign_03.png) no-repeat 0px 10px;background-size: 15px 20px;}



/*这里是头部创作*/
header .right .addmin{display: inline-block;
    float: left;box-sizing: border-box;
    width: 100px;
    height: 80px;
    position: relative;
    padding-top: 20px;}
header .right .addmin p{vertical-align: middle;width:100px;height:40px;line-height: 40px;text-align: center;color:#fff;background: #055cfc;}
header .right .addmin p i{font-size: 20px;margin-right:5px;display: inline-block;}
header .right .addmin p em{line-height: 40px;display: inline-block;vertical-align: top;font-size:15px;}
header .right .addmin span{display: inline-block;width:20px;height:20px;background:#777;color:#c7c7c7;text-align: center;line-height: 20px;position: absolute;top:10px;right:-10px;border-radius: 50%;}



header .right .item{display: inline-block;padding-top:25px;position: relative;margin:0 20px 0;box-sizing: border-box;padding-bottom:20px}
header .right .item img{vertical-align: middle;width:30px;height:30px;padding-bottom:30px}
header .right .item span{background:#055cfc;color:#fff;display: inline-block;width:20px;height:20px;text-align: center;line-height: 20px;position: absolute;top:10px;right:-7px;border-radius: 50%;}


/*这里是消息弹窗*/
header .right .item #new{display: none;width:270px;height:400px;background:#fff;box-shadow:0 0 5px rgba(0,0,0,.7);position: absolute;top:65px;left:-170px;z-index: 999999;}
header .right .item #new .new-box{width:100%;height:100%;position: relative;}
header .right .item #new .new-box .square{display: inline-block;border-bottom:8px solid #fff;border-left:8px dashed transparent;border-right: 8px dashed transparent;border-top:8px solid transparent;position: absolute;top:-16px;left:177px}
header .right .item #new .new-box .new-nav{width:100%;height:100%;display: block;}
header .right .item #new .new-box .new-nav .new-title{width:100%;height:60px;border-bottom: 1px solid #e8e8e8;box-sizing: border-box;text-align: center;line-height: 60px;overflow: hidden;}
header .right .item #new .new-box .new-nav .new-title .message{width:50%;height:100%;display: inline-block;float:left;}
header .right .item #new .new-box .new-nav .new-title .navs{font-size: 18px;position: relative;color:#c8c8c8;display: inline-block;float:left;width:50%;height:100%}
header .right .item #new .new-box .new-nav .new-title .navs i{border-radius: 50%;display: inline-block;width:5px;height:5px;background:#c8c8c8;position: absolute;right:43px;top:20px;color:#c8c8c8}
header .right .item #new .new-box .new-nav .new-title .act{color:#000}
header .right .item #new .new-box .new-nav .new-title .act i{background:#055cfc}

header .right .item #new .new-box .new-nav .new-content{width:100%;overflow-x: hidden;height:340px}
/*这里是消息第一个*/
 header .right .item #new .new-box .new-nav .new-content ul{width:100%;overflow: hidden;display: block;}
 header .right .item #new .new-box .new-nav .new-content ul li{width:100%;height:50px;box-sizing: border-box;border-bottom:1px solid #f2f2f2;text-indent: 50px;font-size: 18px;position: relative;line-height: 50px;}
 header .right .item #new .new-box .new-nav .new-content ul li:hover{background:#eee}
 header .right .item #new .new-box .new-nav .new-content ul.first li i{left:5px}
 header .right .item #new .new-box .new-nav .new-content ul li em{text-indent:0;font-size: 12px;display: inline-block;width:20px;height:20px;border-radius: 50%;background:#055cfc;line-height: 20px;color:#fff;text-align: center;position:absolute;right:15px;top:15px}
 header .right .item #new .new-box .new-nav .new-content ul strong{font-size: 14px;float:right;line-height: 25px;margin:10px 20px 0 0;font-weight: 400;}
 /*这里消息的私信第二个*/
   header .right .item #new .new-box .new-nav .new-content .letter-box{width:220px;margin:0 auto 0;overflow-x:hidden;padding-bottom:20px;display: none;}
    header .right .item #new .new-box .new-nav .new-content .letter-box .letter-item{width:100%;overflow: hidden;margin-top:20px}
    header .right .item #new .new-box .new-nav .new-content .letter-box .letter-item .letter-title{height:40px;overflow: hidden;width:100%}
   header .right .item #new .new-box .new-nav .new-content .letter-box .letter-item .letter-title img{width:40px;height:40px;border-radius: 50%;display: inline-block;float:left;margin-right:10px}
   header .right .item #new .new-box .new-nav .new-content .letter-box .letter-item .letter-title i{display: inline-block;line-height: 40px;font-size: 13px;}
   header .right .item #new .new-box .new-nav .new-content .letter-box .letter-item .letter-new{padding:10px;width:160px;overflow: hidden;font-size: 12px;line-height: 20px;float:right;background:#eee}
   header .right .item #new .new-box .new-nav .new-content .letter-box .letter-item .letter-new i{display: inline-block;font-size: 12px;color:#9f9f9f;line-height: 20px;margin-top:20px}
   header .right .item #new .new-box .new-nav .new-content .letter-box .letter-item .letter-new em{color:#2869fb;display: inline-block;font-size: 12px;line-height: 20px;margin-top:20px;float:right}
   /*这里是主页子菜单里面的通知*/
  .newsub-mater .out-box{width:100%;height:300px;overflow-x:hidden;}
   .newsub-mater .letter-box{width:220px;margin:0 auto 0;padding-bottom:20px;display: block;height:300px}
   .newsub-mater .letter-box .letter-item{width:100%;overflow: hidden;margin-top:20px}
   .newsub-mater .letter-box .letter-item .letter-title{height:40px;overflow: hidden;width:100%}
   .newsub-mater .letter-box .letter-item .letter-title img{width:40px;height:40px;border-radius: 50%;display: inline-block;}
   .newsub-mater .letter-box .letter-item .letter-title i{display: inline-block;line-height: 40px;font-size: 13px;vertical-align: top;}
   .newsub-mater .letter-box .letter-item .letter-new{padding:10px;width:160px;overflow: hidden;font-size: 12px;line-height: 20px;float:right;background:#eee}
   .newsub-mater .letter-box .letter-item .letter-new i{display: inline-block;font-size: 12px;color:#9f9f9f;line-height: 20px;margin-top:20px}
   .newsub-mater .letter-box .letter-item .letter-new strong{display: inline-block;font-size: 12px;color:#fff;line-height: 20px;margin-top:20px;float:right;background:#055cfc;padding:0 5px 0;font-weight: 400;}
   .newsub-mater .letter-box .letter-item .letter-new em{color:#2869fb;display: inline-block;font-size: 12px;line-height: 20px;margin-top:20px;float:right}
  
  
  /*添加好友通知*/
 .newsub-mater .letter-box .mater-item{width:100%;height:70px;overflow: hidden;}
.newsub-mater .letter-box .mater-item .peson-box{width:70px;height:100%;float:left;position: relative;margin-right:5px}
.newsub-mater .letter-box .mater-item .peson-box img{width:40px;height:40px;border-radius: 50%;float:left;margin:15px 0 0 20px}
.newsub-mater .letter-box .mater-item .peson-box i{display: inline-block;width:6px;height:6px;border-radius: 50%;background:#055cfc;position: absolute;top:10px;left:5px}
.newsub-mater .letter-box .mater-item .text-box{box-sizing: border-box;padding-top:10px}
.newsub-mater .letter-box .mater-item .text-box p{white-space:nowrap;overflow: hidden;text-overflow: ellipsis;}
.newsub-mater .letter-box .mater-item .text-box p em{color:#055cfc;margin-right:5px}
.newsub-mater .letter-box .mater-item .text-box a{display: inline-block;font-size: 12px;padding:0 5px;border:1px solid #eee;margin:3px 3px 0 0;background:#f6f6f6;border-radius: 4px;}
.newsub-mater .letter-box .mater-item .text-box a:hover{background:#055cfc;color:#fff}
.newsub-mater .letter-box .mater-item .text-box a.accept{display: none;}
.newsub-mater .letter-box .mater-item .text-box a.reject{display: none;}
  
  
  
  
   
/*这里是消息子菜单小面的材思库*/   
header .right .item #new .new-box .newsub-mater{width:100%;overflow: hidden;display: none;}
header .right .item #new .new-box .newsub-mater .mater-title{width:100%;height:60px;box-sizing: border-box;border-bottom:1px solid #e8e8e8;text-indent: 50px;line-height: 60px;position: relative;font-size: 18px;}
header .right .item #new .new-box .newsub-mater .mater-title i{display: inline-block;position: absolute;left:15px;top:21px;width:22px;height:18px;background:url(../image/newback.png) no-repeat;background-size: 100% 100%;}   
header .right .item #new .new-box .newsub-mater .mater-content{width:100%;overflow-x: hidden;height:300px}   
header .right .item #new .new-box .newsub-mater .mater-content .mater-item{width:100%;overflow: hidden;height:70px;display: flex;position: relative;}
header .right .item #new .new-box .newsub-mater .mater-content .mater-item:hover b{opacity: 1;}
header .right .item #new .new-box .newsub-mater .mater-content .mater-item b{opacity:0;position: absolute;width:15px;height:15px;display: inline-block;right:10px;top:40px;background:url(../image/clear.png) no-repeat;background-size: 100% 100%;} 
header .right .item #new .new-box .newsub-mater .mater-content .mater-item:hover{background:#eee}  
header .right .item #new .new-box .newsub-mater .mater-content .mater-item .peson-box{display: inline-block;width:60px;height:70px;float:left;position: relative;}  
header .right .item #new .new-box .newsub-mater .mater-content .mater-item .peson-box img{width:40px;height:40px;border-radius: 50%;display: inline-block;float:left;margin:15px}
header .right .item #new .new-box .newsub-mater .mater-content .mater-item .peson-box i{position: absolute;width:6px;height:6px;background:#055cfc;border-radius: 50%;left:8px;top:8px}

header .right .item #new .new-box .newsub-mater .mater-content .mater-item .text-box{color:#9a9a9a;display: inline-block;width:60px;float:left;flex:1;font-size: 13px;box-sizing: border-box;overflow: hidden;margin-right:6px}  
header .right .item #new .new-box .newsub-mater .mater-content .mater-item .text-box p{margin:15px 0px 5px;text-overflow: ellipsis;white-space: nowrap;box-sizing: border-box;padding-left:5px}
header .right .item #new .new-box .newsub-mater .mater-content .mater-item .text-box p em{color:#055cfc;}
header .right .item #new .new-box .newsub-mater .mater-content .mater-item .text-box strong{font-weight: 400;text-indent: 10px;display: inline-block;}

header .right .person{display: inline-block;float:right;width:40px;height:40px;position: relative;margin:20px 10px 0;padding-bottom:20px}
header .right .person img{width:40px;height:40px;border-radius: 50%;}
header .right .person strong{display: inline-block;width:20px;height:20px;background:url(../image/vip.png) no-repeat;background-size: 100% 100%;float:left;margin:40px 10px 0;}
header .right .person span{position: absolute;right:0px;bottom:20px;border-bottom:4px solid #999;border-right:4px solid #999;border-top:4px solid transparent;border-left:4px solid transparent}
header .right .person label{float:right;color:#055cfc;margin:15px 15px 0 0;font-size: 12px;}
/*这里是导航人物下面的下拉*/
header .right .person #person{display: none;width:270px;overflow: hidden;height:310px;background:#fff;box-shadow: 0 0 10px rgba(0,0,0,.7);position: absolute;right:0px;top:50px}
header .right .person #person .title{width:100%;height:100px;overflow: hidden;}
header .right .person #person .title img{display: inline-block;float:left;border-radius: 50%;width:60px;height:60px;margin:20px 10px 0}
header .right .person #person .title em.text{display: inline-block;float:left;line-height: 100px;font-size: 18px;font-weight: 600;}
header .right .person #person .title i{float:right;font-size:15px;color:#999;margin:10px;}
header .right .person #person .pson-sub{width:100%;height:60px;border-bottom: 1px solid #dbdbdb;overflow: hidden;box-sizing: border-box;}
header .right .person #person .pson-sub .colect{width:67.5px;height:60px;float:left}
header .right .person #person .pson-sub .colect p{text-align: center;font-size: 15px;margin:5px 0;}
header .right .person #person .pson-sub .colect em{text-align: center;font-size: 13px;display: block;}
header .right .person #person .pson-sub .datas{width:90px;height:60px;float:left}
header .right .person #person .pson-sub .flow{width:90px;height:60px;float:left}
header .right .person #person .list{width:100%;overflow: hidden;padding-top:15px}
header .right .person #person .list a{display: block;text-indent: 30px;line-height: 25px;font-size:13px}
header .right .person #person .list a:hover{background:#f2f2f2}
/*这是头部导航材思库 下面的按钮*/
header .right .addmin .library{position: absolute;width:150px;box-sizing: border-box;top:70px;left:-25px;z-index: 100;background: #fff;box-shadow: 0 0 10px rgba(0,0,0,.7);display: none;}
header .right .addmin .library ul{width:100%;}
header .right .addmin .library .libra-box{position: relative;}
header .right .addmin .library .libra-box strong{display: inline-block; position: absolute;top:-16px;left:67px;   border-bottom: 8px solid #fff; border-left: 8px dashed transparent; border-right: 8px dashed transparent;
    border-top: 8px solid transparent;}
header .right .addmin .library ul li{width:100%;height:60px;box-sizing: border-box;border-bottom:1px solid #eee;line-height: 60px;color:#000;text-indent:50px;font-size: 15px;}
header .right .addmin .library ul li.creater{background:url(../image/creation_03.png) no-repeat 15px center;background-size: 27px 25px;}
header .right .addmin .library ul li.product{background:url(../image/creation_06.png) no-repeat 15px center;background-size: 27px 25px;}
header .right .addmin .library ul li.Release{background:url(../image/creation_08.png) no-repeat 15px center;background-size: 27px 25px;}
/*这里是分享弹窗*/
#share{width:100%;height:100%;background:rgba(0,0,0,.8);position: absolute;top:0;z-index:999999;left:0;display: none;}
#share .play-share{width:100%;height:100%;position: relative;}
