/*这里是添加材思库弹窗*/
#library{width:100%;height:100%;position: absolute;left:0;top:0;background:rgba(0,0,0,.8);z-index: 99999999;display: none;}
#library .library-box{width:100%;height:100%;position:relative}
#library .library-box .library-content{width:700px;height:550px;background:#fff;position: absolute;transform: translate(-50%,-50%);left:50%;top:50%}
#library .library-box .library-content .library-title{width:100%;height:60px;border-bottom: 1px solid #eee;line-height: 60px;box-sizing: border-box;}
#library .library-box .library-content .library-title span{display: inline-block;float:left;line-height: 60px;margin-left:40px;color:#646464;font-size: 20px;font-weight: 600;}
#library .library-box .library-content .library-title i{display: inline-block;float:right;background:url(../image/delete_03.png) no-repeat;background-size: 100% 100%;width:16px;height:16px;margin:22px 20px 0}
#library .library-box .library-content .library-show{overflow: hidden;}
#library .library-box .library-content .library-show .library-left{width:250px;display: inline-block;float:left;height:490px;border-right: 1px solid #eee;box-sizing: border-box;}
#library .library-box .library-content .library-show .library-left img{display: block;width:180px;height:180px;margin:30px auto 0}
#library .library-box .library-content .library-show .library-left h2{font-size: 20px;;text-indent: 40px;margin:16px 0 0}
#library .library-box .library-content .library-show .library-left p{font-size: 14px;line-height:25px;width:70%;margin:10px auto 0;text-overflow: ellipsis;    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-line-clamp: 7;
    /*height: 0.4rem;*/
   height:200px;
    -webkit-box-orient: vertical}
#library .library-box .library-content .library-show .library-right{width:450px;display: inline-block;float:left;position: relative;height:490px}
#library .library-box .library-content .library-show .library-right .output{overflow: hidden;}
#library .library-box .library-content .library-show .library-right .output input{display: block;width:80%;height:30px;box-sizing: border-box;outline: none;border:1px solid #999;text-indent: 15px;margin:20px auto 10px}
#library .library-box .library-content .library-show .library-right .select{width:80%;overflow: hidden;margin: 0 auto 0;height:225px}
#library .library-box .library-content .library-show .library-right .select p{font-size:20px;color:#abaaaa;line-height: 25px;}
#library .library-box .library-content .library-show .library-right .select-box{width:100%;height:200px;overflow-y: scroll;}
#library .library-box .library-content .library-show .library-right .select-box .item{border-radius: 5px;width:95%;background:#eee;font-size: 20px;color#000;line-height: 40px;text-indent: 20px;margin:10px 0 0;height:40px}
#library .library-box .library-content .library-show .library-right .label{width:80%;overflow: hidden;margin: 10px auto 0}
