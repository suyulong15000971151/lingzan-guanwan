/*成功的弹窗*/
#win{width:100%;height:100%;position: absolute;top:0;left;0;background:rgba(0,0,0,.6);z-index: 999999;display: none;}
.anima{animation: alls 0.5s ease-in-out;}
#win .win-box{width:100%;height:100%;position: relative;}
#win .win-box .com{width:250px;height:100px;background:#fff;position: absolute;top:50%;left:50%;transform: translate(-50%,-50%);}
#win .win-box .com img{width:40px;height:40px;float:left;margin:30px 100px;}
#win .win-box .com i{display: inline-block;width:12px;height:12px;position: absolute;top:10px;right:10px;background:url(../image/delete_03.png) no-repeat;background-size: 100% 100%;float:right}
#win .win-box .com span{text-align: center;}


/*这里是确认上传弹窗*/
#config{position: fixed;width:100%;height:100%;background:rgba(0,0,0,.4);z-index: 99999999999;top:0;left:0;transform: scale(0);}
#config .config-box{width:100%;height:100%;position: relative;}
#config .config-box .content{width:200px;height:100px;background:#fff;position: absolute;top:50%;left:50%;transform: translate(-50%,-50%);border-radius: 5px;}
#config .config-box .content .title{width:190px; height:38px; margin:10px 5px; color:#a2a2a2;text-align: center;float:left;}
#config .config-box .content button{width:60px;height:30px;background: #47a447;color:#fff;line-height: 30px;border-radius: 2px;text-align: center;margin:0 10px 0 10px;border:none;outline: none;}
#config .config-box .content span{width:60px;height:30px;display: inline-block;background: #47a447;color:#fff;line-height: 30px;border-radius: 2px;text-align: center;margin:0 10px 0 30px;float:left;}
div.big{transition:transform 0.5s linear;transform: scale(1) !important;}
@keyframes win{
      0%{transform: scale(0);}
      100%{transform: scale(1)};
}
@keyframes big{
      0%{transform: scale(0);}
      100%{transform: scale(1)};
}

@keyframes rota{
      from{transform: rotate(360deg);transform-origin: center;}
      to{transform: rotate(0deg);transform-origin: center;}
}

@keyframes k-loadingO {
      0%,100% {
      transform:translate(0)
}
25% {
      transform:translate(40px)
}
50% {
      transform:translate(40px,40px)
}
75% {
      transform:translate(0,40px)
}

@keyframes alls{
      0%{transform: scale(0);}
      100%{transform: scale(1);}
      /*100%{transform: scale(0);}*/
}